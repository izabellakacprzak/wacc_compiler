# Function to find the maximum positive value in an array of ints
# Returns the smallest integer value (-2147483648) if array is empty

begin
  int max(int[] arr) is
    int length = len arr ;
    int max = -2147483648 ;
    int i = 0 ;
    while i < length do
      if arr[i] > max
      then
        max = arr[i]
      else
        skip
      fi ;
      i = i + 1
    done ;
    return max
  end
  skip
end
