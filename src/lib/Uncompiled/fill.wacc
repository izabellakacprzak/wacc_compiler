# Functions to fill an array with the same inputted value

begin
  int[] fill_int(int val, int[] arr) is
    int length = len arr ;
    int i = 0 ;
    while i < length do
      arr[i] = val ;
      i = i + 1
    done ;
    return arr
  end
  char[] fill_char(char val, char[] arr) is
    int length = len arr ;
    int i = 0 ;
    while i < length do
      arr[i] = val ;
      i = i + 1
    done ;
    return arr
  end
  skip
end
