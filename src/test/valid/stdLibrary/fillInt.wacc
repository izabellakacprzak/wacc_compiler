# using the fill_int function from the standard library

# Output:
# [1, 1, 1, 1]

# Program:

begin
  int[] arr = [4, 2, 7, 5] ;
  arr = call fill_int(1, arr) ;
  
  print "[" ;
  int i = 0 ;
  int length = len arr ;
  while i < (length - 1) do
    print arr[i] ;
    print ", " ;
    i = i + 1
  done ;
  print arr[i] ;
  println "]"
end
