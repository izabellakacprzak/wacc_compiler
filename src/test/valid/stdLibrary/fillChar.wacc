# using the fill_char function from the standard library

# Output:
# [a, a, a, a]

# Program:

begin
  char[] arr = ['q', 'u', 'd', 'k'] ;
  arr = call fill_char('a', arr) ;
  
  print "[" ;
  int i = 0 ;
  int length = len arr ;
  while i < (length - 1) do
    print arr[i] ;
    print ", " ;
    i = i + 1
  done ;
  print arr[i] ;
  println "]"
end
