# using the swap_char function from the standard library

# Output:
# [c, b, a, d]

# Program:

begin
  char[] arr = ['a', 'b', 'c', 'd'] ;
  arr = call swap_char(0, 2, arr) ;
  
  print "[" ;
  int i = 0 ;
  int length = len arr ;
  while i < (length - 1) do
    print arr[i] ;
    print ", " ;
    i = i + 1
  done ;
  print arr[i] ;
  println "]"
end
