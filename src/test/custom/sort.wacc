# using the sort function from the standard library

# Output:
# [2, 3, 4, 7]

# Program:

begin
  int[] arr = [7, 3, 4, 2] ;
  arr = call sort(arr) ;
  
  print "[" ;
  int i = 0 ;
  int length = len arr ;
  while i < (length - 1) do
    print arr[i] ;
    print ", " ;
    i = i + 1
  done ;
  print arr[i] ;
  println "]"
end
